{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react-native",
    "lib": [
      "es6",
      "esnext.asynciterable",
      "dom"
    ],
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "types": [],
  "typeRoots": [
    "./src/types"
  ],
  "files": [
    "./src/types/index.d.ts"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
